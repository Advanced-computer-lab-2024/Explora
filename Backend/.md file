{
  "_id": "6748fae835a3fa0479d4a088",
  "username": "mimi",
  "email": "new_email@example.com",
  "password": "$2b$10$hashed_password_string",
  "name": "New Name",
  "mobile": "0123456789",
  "yearsOfExperience": 5,
  "previousWork": "New Work Details",
